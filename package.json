  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "jquery": "^3.1.1",
    "jshint": "^2.13.1",
    "steal": "^0.16.45",
    "steal-mocha": "0.0.3",
    "testee": "^0.9.1"
  }
